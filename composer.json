{
	"name" : "phootwork/file",
	"type" : "library",
	"description" : "File Operations for the local Filesystem",
	"authors" : [{
			"name" : "Thomas Gossmann",
			"homepage" : "http://gos.si"
		}
	],
	"license" : "MIT",
	"keywords" : [
		"file",
		"filesystem"
	],
	"support" : {
		"issues" : "https://github.com/phootwork/file/issues"
	},
	"autoload" : {
		"psr-4" : {
			"phootwork\\file\\" : "src"
		}
	},
	"autoload-dev" : {
		"psr-4" : {
			"phootwork\\file\\tests\\" : "tests"
		}
	},
	"require" : {
		"php" : ">=5.6",
		"phootwork/lang" : "~0"
	},
	"require-dev" : {
		"phpunit/phpunit" : "^5.7",
		"sami/sami" : "^4|^3",
		"mikey179/vfsStream" : "^1"
	},
	"scripts" : {
		"test" : "phpunit",
		"api" : "sami.php update sami.php"
	}
}